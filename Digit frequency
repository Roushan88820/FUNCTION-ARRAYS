import java.util.*;
public class Main {
  public static void main(String[]args) {
    Scanner scn = new Scanner(System.in);
    int n = scn.nextInt();
    int d = scn.nextInt();
   
   int result = count_digit(n,d);
   System.out.println(result);
  }
  public static int count_digit(int n,int d){
       int count_digit = 0;
    while (n > 0) {
      int rem = n % 10;
      if (rem == d) {
        count_digit++;
      }

      n = n / 10;


    }
    return count_digit;
  }
}
